<?php

/**
 * @file
 * Contains \Drupal\employee\employee.module.
 */

/**
 * Implements hook_page_attachments().
 */
function employee_page_attachments(array &$attachments) {
  $attachments['#attached']['library'][] = 'employee/css-library';  
  $attachments['#attached']['library'][] = 'employee/js-library';
    
}
